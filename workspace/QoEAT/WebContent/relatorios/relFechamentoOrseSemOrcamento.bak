	<parameter name="MESANOREFERENCIA" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["and  docu.ano || '/' || substring(docu.mes+100,2,2) = '2010/04' "]]></defaultValueExpression>
	</parameter>
	<parameter name="SQLDATA2" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="SQLDATA0" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[", to_char(docu.datasituacao, 'DD/MM/YYYY')"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  case when (select count(peju.*) from pessoajuridica peju where pess.id = peju.pessid )>0 then (select peju.nomefantasia from pessoajuridica peju where pess.id = peju.pessid) else pess.nome end as cliente
,	orse.titulo as titulo
,	orse.formato as formato
,	peca.nome as peca
,	substring(docubrie.numero+1000,2,3) || '/' || substring(docubrie.mes+100,2,2) || docubrie.ano as briefing
,	substring(docu.numero+1000,2,3) || '/' || substring(docu.mes+100,2,2) || docu.ano as numero
,	docu.ano || '/' || substring(docu.mes+100,2,2) || substring(docu.numero+1000,2,3) as numeroordem
,	sido.abreviatura as situacao
from    documento docu
,	situacaodocumento sido
,	ordemservico orse
,	peca
,	briefing brie
,	documento docubrie
,	cliente clie
,	pessoa pess
$P!{SQLDATA}
where   docu.sidoid = sido.id
and     docu.id = orse.docuid
and     orse.pecaid = peca.id
and     orse.docuidbrie = brie.docuid
and     brie.docuid = docubrie.id
and     brie.pessidclie = clie.pessid
and     clie.pessid = pess.id
and     not exists ( select * from orcamento orca where docu.id = orca.docuidorse)
and     docu.pessidempr = $P!{PESSIDEMPR}
$P!{SQLSIDO}
$P!{PESSIDCLIE}
$P!{PECAID}
$P!{ATIVID}
$P!{USUAIDATENDENTE}
$P!{SQLDATA2}
$P!{MESANOREFERENCIA}
$P!{SQLFINANCEIROABERTO}
group by  pess.id
,	  pess.nome
,	  orse.formato
,	  orse.titulo 
,	  substring(docu.numero+1000,2,3) || '/' || substring(docu.mes+100,2,2) || docu.ano
,	  docu.ano || '/' || substring(docu.mes+100,2,2) || substring(docu.numero+1000,2,3)
$P!{SQLDATA0}
,	  sido.id
,	  sido.nome
,	  peca.nome
,	  docu.id
,	  docubrie.numero
,	  docubrie.mes
,	  docubrie.ano
,	  docu.numero
,	  sido.abreviatura
order by  numeroordem]]></queryString>

