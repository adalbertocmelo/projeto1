	<queryString><![CDATA[select  to_char(nofi.dataemissao,'DD/MM/YYYY') as dataemissao
,	nofi.valortotal
,	nofi.numeronofi 
,	nofi.numeronofi || '/' ||to_char(nofi.dataemissao,'YYYY') as numeroduplicata
,	to_char(nofi.datavencimento,'DD/MM/YYYY') as datavencimento
,	pess.nome as clienome
,	pess.logradouro
,	substring(pess.cep,1,5) || '-' || substring(pess.cep,6,3) as cep
,	cida.nome as cidanome
,	esta.uf as estauf
,	(select substring(peju.cnpj,1,2) || '.' || substring(peju.cnpj,3,3) || '.' || substring(peju.cnpj,6,3) || '/' || substring(peju.cnpj,9,4) || '-' || substring(peju.cnpj,13,2) from pessoajuridica peju where pess.id = peju.pessid union all select substring(pefi.cpf,1,3) || '.' || substring(pefi.cpf,4,3) || '.' || substring(pefi.cpf,7,3) || '-' || substring(pefi.cpf,10,2) from pessoafisica pefi where pess.id = pefi.pessid) as cnpjcpf
,	(select peju.inscricaoestadual from pessoajuridica peju where pess.id = peju.pessid) as inscricaoestadual
,	itnf.descricao as itemdescricao
,	itnf.valornaotributado
from    notafiscal nofi
,	documento docunofi
,	itemnotafiscal itnf
,	pessoa pess
,	cidade cida
,	estado esta
where   nofi.docuid = docunofi.id
and     nofi.pessidclie = pess.id
and     pess.cidaid = cida.id
and     cida.estaid = esta.id
and     itnf.docuidnofi = nofi.docuid
$P!{DOCUID}]]></queryString>

