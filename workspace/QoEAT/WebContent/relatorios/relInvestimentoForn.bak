	<queryString><![CDATA[select     pessclie.nome as clinome
,    pessforn.nome as fornecedor
,    sum(itap.quantidade * itap.valor) as total
from    documento docuaupr
,    autorizacaoproducao aupr
,    itemautorizacaoproducao itap
,    orcamento orca
,    ordemservico orse
,    briefing brie
,    cliente clie
,    pessoa pessclie
,    fornecedor forn
,    pessoa pessforn
$P!{SQLDATA1}
where   docuaupr.id = aupr.docuid
and     aupr.docuid = itap.docuidaupr
and     orca.docuid = aupr.docuidorca
and     orse.docuid = orca.docuidorse
and     brie.docuid = orse.docuidbrie
and     pessclie.id = clie.pessid
and     brie.pessidclie = clie.pessid
and     pessforn.id = forn.pessid
and     aupr.pessidforn = forn.pessid
and     docuaupr.pessidempr = $P{PESSIDEMPR}
$P!{PESSIDCLIE}
$P!{SQLDATA2}
group by pessclie.nome
,    pessforn.nome]]></queryString>

