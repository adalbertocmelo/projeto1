	</parameter>
	<parameter name="SQLDATA2" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["and     docuaupr.id = primeirohsid.docuid  and      to_char(primeirohsid.datacadastro, 'YYYY-MM-DD') >= '2009-01-01' and      to_char(primeirohsid.datacadastro, 'YYYY-MM-DD') <= '2009-09-01'"]]></defaultValueExpression>
