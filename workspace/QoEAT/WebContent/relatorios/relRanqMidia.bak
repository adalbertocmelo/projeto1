	<queryString><![CDATA[select 	case when (select count(peju.*) from pessoajuridica peju where pessclie.id = peju.pessid )>0 then (select peju.nomefantasia from pessoajuridica peju where pessclie.id = peju.pessid) else pessclie.nome end as clinome
,	sum(aumi.customidia) as total
from    documento docu
,	autorizacaomidia aumi
,	mapamidia mami
,	ordemservico orse
,	briefing brie
,	cliente clie
,	pessoa pessclie
$P!{SQLDATA}
where   docu.id = aumi.docuid
and     mami.docuid = aumi.docuidmami
and     orse.docuid = mami.docuidorse
and     brie.docuid = orse.docuidbrie
and     pessclie.id = clie.pessid
and     brie.pessidclie = clie.pessid
and     docu.sidoid <> 3
and	docu.pessidempr = $P!{PESSIDEMPR}
$P!{MESANOREFERENCIA}
$P!{SQLDATA2}
$P!{USUAIDATENDENTE}
group by pessclie.id, pessclie.nome
order by total desc]]></queryString>

