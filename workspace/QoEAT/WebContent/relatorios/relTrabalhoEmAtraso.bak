	</parameter>
	<queryString><![CDATA[select  unor.ancestrais||'/'||unor.nome as departamento
,	trab.id as numero
,	trab.descricao as trabalho
,	to_char(extr.inicio, 'DD/MM/YYYY HH24:MI') as inicio
,	tare.nome as tarefa
,	situ.nome as situacao
,	usua.nome as responsavel
,	case when tare.duracao < interval '1 day' then to_char(tare.duracao, 'HH24:MI') else to_char(tare.duracao, 'DD "dias" HH24:MI') end as duracao
,	to_char(extr.inicio + tare.duracao, 'DD/MM/YYYY HH24:MI') as prazofinal
,	case when current_timestamp - (extr.inicio + tare.duracao) < interval '1 day' then to_char(current_timestamp - (extr.inicio + tare.duracao), 'HH24:MI') else to_char(current_timestamp - (extr.inicio + tare.duracao), 'DD "dias" HH24:MI') end as atraso
from 	unidadeorganizacional unor
,	execucaotrabalho extr
,	trabalho trab
,	tarefa tare
,	situacao situ
,	usuarios usua
where	extr.unorid = unor.id
and	extr.trabid = trab.id
and	extr.tareid = tare.id
and	extr.situid = situ.id
and	extr.usuaid = usua.id
and	unor.hierarquico like ($P!{UNORID} + 1000) || '%'
and	duracao < current_timestamp
$P!{TRABID}]]></queryString>

