	<parameter name="DOCUID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="EMPRID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["157"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  aupr.docuid
,	substring(docu.numero+1000,2,3) || '/' || substring(docu.mes+100,2,2) || docu.ano as auprnumero
, 	case when (select count(peju.*) from pessoajuridica peju where forn.id = peju.pessid )>0 then (select peju.nomefantasia from pessoajuridica peju where forn.id = peju.pessid) else forn.nome end as fornnome
,	substring(docuorca.numero+1000,2,3) || '/' || substring(docuorca.mes+100,2,2) || docuorca.ano as orcanumero
,	substring(docuorse.numero+1000,2,3) || '/' || substring(docuorse.mes+100,2,2) || docuorse.ano as orsenumero
, 	case when (select count(peju.*) from pessoajuridica peju where clie.id = peju.pessid )>0 then (select peju.nomefantasia from pessoajuridica peju where clie.id = peju.pessid) else clie.nome end as clienome
,	case when orse.titulocriacao is not null then orse.titulocriacao || ', ' else '' end  || orse.titulo as orsetitulo
, 	sido.abreviatura as sidoabreviatura
, 	ativ.nome as ativnome
from    autorizacaoproducao aupr
,	documento docu
,	orcamento orca
,	documento docuorca
,	ordemservico orse
,	documento docuorse
,	briefing brie
,	documento docubrie
,	pessoa forn
,	pessoa clie
,	situacaodocumento sido
,	(select true as id, 'Sim' as nome union all select false as id, 'Não' as nome) ativ
where 	aupr.docuid = docu.id
and 	aupr.docuidorca = orca.docuid
and 	orca.docuid = docuorca.id
and 	orca.docuidorse = orse.docuid
and 	orse.docuid = docuorse.id
and 	orse.docuidbrie = brie.docuid
and 	brie.docuid = docubrie.id
and 	brie.pessidclie = clie.id
and 	aupr.pessidforn = forn.id
and 	docu.sidoid = sido.id
and 	orca.ativo = ativ.id
$P!{DOCUID}
and     docu.pessidempr = $P{EMPRID}]]></queryString>

