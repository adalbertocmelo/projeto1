	<parameter name="PESSIDCLIE" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[" and true"]]></defaultValueExpression>
	</parameter>
	<parameter name="SQLDATA1" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[",    (select docuid    ,    min(datahora) as datacadastro     from    historicosituacaodocumento hsid     group by docuid) primeirohsid"]]></defaultValueExpression>
	</parameter>
	<parameter name="SQLDATA2" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["and     docuorca.id = primeirohsid.docuid  and      to_char(primeirohsid.datacadastro, 'YYYY-MM-DD') >= '2009-01-01' and      to_char(primeirohsid.datacadastro, 'YYYY-MM-DD') <= '2009-09-01'"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 	pessclie.nome as cliente
,	peca.nome as pecanome
,	esta.nome as estado 
,	sum((itor.quantidade * itor.valor) - (itor.quantidade * itor.valor * itor.desconto / 100)) as orcitem
from    documento docuorca
,	orcamento orca
,	itemorcamento itor
,	ordemservico orse
,	briefing brie
,	cliente clie
,	pessoa pessclie
,	peca peca
,	cidade cida
,	estado esta
$P!{SQLDATA1}
where   docuorca.id = orca.docuid
and     itor.docuidorca = orca.docuid
and     orse.docuid = orca.docuidorse
and     brie.docuid = orse.docuidbrie
and     pessclie.id = clie.pessid
and     brie.pessidclie = clie.pessid
and     orse.pecaid = peca.id
and     orca.cidaid = cida.id
and     cida.estaid = esta.id
and	docuorca.pessidempr = $P{PESSIDEMPR}
$P!{PESSIDCLIE}
$P!{SQLDATA2}
group by pessclie.nome
,	 peca.nome
,	 esta.nome 
order by cliente
,	estado]]></queryString>

