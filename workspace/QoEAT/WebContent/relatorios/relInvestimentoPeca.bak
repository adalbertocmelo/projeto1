		<defaultValueExpression ><![CDATA["and docu.ano || '/' || substring(docu.mes+100,2,2) = '2010/07' "]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 	peca.nome as pecanome
,	sum((itor.quantidade * itor.valor) - (itor.quantidade * itor.valor * itor.desconto / 100)) as orcitem
from    documento docu
,	orcamento orca
,	itemorcamento itor
,	ordemservico orse
,	briefing brie
,	cliente clie
,	pessoa pessclie
,	peca peca
$P!{SQLDATA}
where   docu.id = orca.docuid
and     itor.docuidorca = orca.docuid
and     orse.docuid = orca.docuidorse
and     brie.docuid = orse.docuidbrie
and     pessclie.id = clie.pessid
and     brie.pessidclie = clie.pessid
and     orse.pecaid = peca.id
and     docu.pessidempr = $P{PESSIDEMPR}
$P!{PESSIDCLIE}
$P!{SQLDATA2}
$P!{MESANOREFERENCIA}
group by peca.nome
order by orcitem desc]]></queryString>

