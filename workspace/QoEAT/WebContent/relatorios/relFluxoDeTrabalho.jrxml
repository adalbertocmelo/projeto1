	<parameter name="FLTRID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["and fltr.id = 36"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  titr.nome as titrnome
,	fltr.nome as fluxo
,	unor.ancestrais||'/'||unor.nome as departamento
,	case when ftta.duracao < interval '1 day' then to_char(ftta.duracao, 'HH24:MI') else to_char(ftta.duracao, 'DD "dias" HH24:MI') end as duracao
,	ftta.ordem
,	tare.nome as tarefa
,	situ.nome as situacao
,	usua.nome as responsavel
from 	fluxotrabalho fltr
,	tipotrabalhofluxotrabalho ttft
,	tipotrabalho titr
,	unidadeorganizacional unor
,	fluxotrabalhotarefa ftta
,	situacaotarefa sita
,	tarefa tare
,	situacao situ
,	usuarios usua
where	fltr.id = ftta.fltrid
and	ftta.tareid = tare.id
and	ftta.unorid = unor.id
and	tare.id = sita.tareid
and	sita.situid = situ.id
and	ftta.usuaid = usua.id
and     fltr.id = ttft.fltrid
and     titr.id = ttft.titrid
$P!{FLTRID}
union all
select  titr.nome as titrnome
,	fltr.nome as fluxo
,	unor.ancestrais||'/'||unor.nome as departamento
,	case when ftta.duracao < interval '1 day' then to_char(ftta.duracao, 'HH24:MI') else to_char(ftta.duracao, 'DD "dias" HH24:MI') end as duracao
,	ftta.ordem
,	tare.nome as tarefa
,	situ.nome as situacao
,	'' as responsavel
from 	fluxotrabalho fltr
,	tipotrabalhofluxotrabalho ttft
,	tipotrabalho titr
,	unidadeorganizacional unor
,	fluxotrabalhotarefa ftta
,	situacaotarefa sita
,	tarefa tare
,	situacao situ
where	fltr.id = ftta.fltrid
and	ftta.tareid = tare.id
and	ftta.unorid = unor.id
and	tare.id = sita.tareid
and	sita.situid = situ.id
and	ftta.usuaid is null
and     fltr.id = ttft.fltrid
and     titr.id = ttft.titrid
$P!{FLTRID}
order by titrnome, fluxo, ordem, tarefa, situacao]]></queryString>

