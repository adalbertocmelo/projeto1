	<parameter name="EMPRID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["157"]]></defaultValueExpression>
	</parameter>
	<parameter name="DOCUID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  orca.docuid
,	substring(docu.numero+1000,2,3) || '/' || substring(docu.mes+100,2,2) || docu.ano as orcanumero
,	substring(docuorse.numero+1000,2,3) || '/' || substring(docuorse.mes+100,2,2) || docuorse.ano as orsenumero
, 	case when (select count(peju.*) from pessoajuridica peju where pess.id = peju.pessid )>0 then (select peju.nomefantasia from pessoajuridica peju where pess.id = peju.pessid) else pess.nome end as clienome
,	case when orse.titulocriacao is not null then orse.titulocriacao || ', ' else '' end  || orse.titulo as orsetitulo
, 	peca.nome as pecanome
, 	sido.abreviatura as sidoabreviatura
, 	ativ.nome as ativnome
,	(select sum((itor.valor*itor.quantidade) - ((itor.valor*itor.quantidade) * case when (itor.desconto is null) or (itor.desconto = 0) then 0 else (itor.desconto/100) end))::numeric(15,2) as total
	from   itemorcamento itor 
	where 	itor.docuidorca = orca.docuid) as valor
from    orcamento orca
,	documento docu
,	documento docuorse
,	ordemservico orse
,	briefing brie
,	documento docubrie
,	cliente clie
,	pessoa pess
,	peca
,	situacaodocumento sido
,	cidade cida
,	(select true as id, 'Sim' as nome union all select false as id, 'Não' as nome) ativ
where 	orca.docuid = docu.id
and 	orca.docuidorse = orse.docuid
and 	orse.docuid = docuorse.id
and 	orse.docuidbrie = docubrie.id
and 	brie.docuid = docubrie.id
and 	clie.pessid = pess.id
and 	brie.pessidclie = clie.pessid
and 	orse.pecaid = peca.id
and 	docu.sidoid = sido.id
and 	orca.cidaid = cida.id
and 	orca.ativo = ativ.id
$P!{DOCUID}
and     docu.pessidempr = $P{EMPRID}]]></queryString>

